og(this.state)\n\n      return (\n        <div>\n         <Card >\n          <Card.Content  header={this.props.note.title} />\n          <Card.Content description={this.props.note.content} />\n          <Card.Content extra>\n          <Button onClick={this.handleView} inverted color='violet' size='large'>View</Button>\n          <Button onClick={this.handleEdit} inverted color='purple' size='large' >Edit</Button>\n          <Button onClick={this.handleDelete} inverted color='pink' size='large' >Delete</Button>\n\n          </Card.Content>\n        </Card>\n         <ShowNote selectedNote={this.state} deleteNote={this.props.deleteNote}/>\n        </div>\n      );\n    }\n  }\n  \n  export default NotesCard;\n\n  // {this.props.note.title}: \n  //        {this.props.note.content}\n  //        <button onClick={this.handleView} > View </button>\n  //        <button onClick={this.handleEdit}> edit</button>\n","/Users/ronalyssa/Flatiron/mod4/project4/notes-client/src/LogIn.js",["66"],"import React, { Component } from 'react';\nimport { Link } from 'react-router-dom'\n\nclass LogIn extends Component {\n    render() {\n      return (\n       <div>\n           Login Page\n           <button>Username</button>\n           <button>Login</button>\n       </div>\n      \n      );\n    }\n  }\n  \n  export default LogIn;","/Users/ronalyssa/Flatiron/mod4/project4/notes-client/src/ShowNote.js",[],"/Users/ronalyssa/Flatiron/mod4/project4/notes-client/src/EditForm.js",[],{"ruleId":"67","replacedBy":"68"},{"ruleId":"69","replacedBy":"70"},{"ruleId":"71","severity":1,"message":"72","line":4,"column":3,"nodeType":"73","messageId":"74","endLine":4,"endColumn":9},{"ruleId":"71","severity":1,"message":"75","line":5,"column":3,"nodeType":"73","messageId":"74","endLine":5,"endColumn":8},{"ruleId":"71","severity":1,"message":"76","line":6,"column":3,"nodeType":"73","messageId":"74","endLine":6,"endColumn":7},{"ruleId":"71","severity":1,"message":"77","line":14,"column":8,"nodeType":"73","messa